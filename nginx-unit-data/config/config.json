{
  "listeners": { "*:8080": { "pass": "routes" } },
  "routes": [
    {
      "match": { "uri": "/static/*" },
      "action": { "share": "/code/example/" }
    },
    { "match": { "uri": "/media/*" }, "action": { "share": "/code/example/" } },
    { "action": { "pass": "applications/django-app" } }
  ],
  "applications": {
    "django-app": {
      "type": "python 3",
      "path": "/code/example/",
      "module": "example.wsgi",
      "environment": { "DJANGO_SETTINGS_MODULE": "example.settings" }
    }
  }
}
